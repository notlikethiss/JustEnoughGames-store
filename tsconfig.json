{
  "compilerOptions": {    
    "lib": ["dom", "dom.iterable", "esnext"],    
    "target": "ESNext",    
    "types": ["vite/client"],
    "isolatedModules": true,
    "strict": true,
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "noEmit": true,
    "jsx": "react-jsx"
  },
  "include": [
    "src"
  ]
}
